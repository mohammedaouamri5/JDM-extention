{
	"manifest_version": 2,
 
 	"name": "__MSG_extensionName__",
	"version": "1.5.4",
	"description": "__MSG_extensionDescription__",

	"default_locale": "en",
 
 	"icon s": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png"
	},

	"permissions": [
		"activeTab",
		"bookmarks",
		"contextMenus",
		"storage",
		"tabs"
	],

	"background": {
		"scripts": [
 			"content/background/background.js"
		]
	},

	"browser_action": {
		"default_icon": {
			"16": "icons/icon16.png",
			"32": "icons/icon32.png",
			"48": "icons/icon48.png"
		},

		"default_title": "__MSG_extensionName__",
		"default_popup": "content/button/panel.html"
	}
	
 
}
